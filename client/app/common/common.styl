@import url(http://fonts.googleapis.com/css?family=Nunito)

$accentColor = #f45e29
$bgColor = #26c9da
$darkBgColor = #22b4c3
$textColor = #ffffff

body
  height 100%
  // browsers default to 16px for font-size,
  // so setting top-level font-size to 62.5%
  // scales 1rem to equal 10px.
  font 62.5% 'Nunito', sans-serif
